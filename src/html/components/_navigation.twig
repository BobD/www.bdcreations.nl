<nav data-js="navigation" class="navigation {{modifiers}}">
	<ul class="navigation__list">
		{% for item in navigation.site %}
			<li data-js="navigation__item" class="navigation__item" data-id="{{item.id}}">
				<a href="{{item.url}}" class="navigation__link">
					<span class="navigation__label">{{item.label}}</span>
				</a>
			</li>
		{% endfor %}
	</ul>
</nav>