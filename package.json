{
  "name": "bdcreations",
  "version": "0.0.1",
  "description": "frontend for new bdcreations website",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build/* && gulp --env development && webpack -d --ENV=development",
    "build:prod": "rimraf dist/* && gulp --env production && webpack -p --ENV=production",
    "watch": "gulp watch --env development",
    "serve": "webpack-dev-server --inline --hot --open --content-base build/ --ENV=development"
  },
  "keywords": [
    "bdcreations"
  ],
  "author": "Bob Donderwinkel",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "cssnano": "^3.7.6",
    "del": "^2.2.2",
    "front-matter": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-data": "^1.2.1",
    "gulp-if": "^2.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-postcss": "^6.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-twig": "^0.5.0",
    "minimist": "^1.2.0",
    "parallelshell": "^2.0.0",
    "postcss": "^5.2.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-utilities": "^0.5.1",
    "rimraf": "^2.5.4",
    "underscore": "^1.8.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "yargs": "^6.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0"
  }
}
